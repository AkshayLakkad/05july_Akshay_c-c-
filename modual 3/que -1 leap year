#include<stdio.h>
#include<conio.h>
void main()
{
  int year;
  clrscr();
  printf("Enter A Year");
  scanf("%d",&year);
  if(year %400==0)
  {
  printf("%d is Leap Year",year);
  }
  else if(year %100==0)
  {
  printf("%d is Not Leap Year",year);
  }
  else if(year %4==0)
  {
  printf("%d is Leap Year",year);
  }
  else
  {
  printf("%d is Not Leap Year",year);
}
getch();
}
